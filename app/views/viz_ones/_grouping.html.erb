<div class="side-bar-item grouping">
  <h2>Grouping</h2>
  <div class="grouping-list">
    <label class="radio-container">None
      <input type="radio" checked="checked" name="grouping" id="grouping-none" value="none">
      <span class="radio-checkmark"></span>
    </label>
    <label class="radio-container">Baseline result
      <input type="radio" name="grouping" id="grouping-baseline" value="baseline_group">
      <span class="radio-checkmark"></span>
    </label>
    <label class="radio-container">Diabetes type
      <input type="radio" name="grouping" id="grouping-dmt" value="diabetes_type">
      <span class="radio-checkmark"></span>
    </label>
    <label class="radio-container">Ethnicity
      <input type="radio" name="grouping" id="grouping-ethnicity" value="ethnicity">
      <span class="radio-checkmark"></span>
    </label>
    <label class="radio-container">Sex
      <input type="radio" name="grouping" id="grouping-sex" value="sex">
      <span class="radio-checkmark"></span>
    </label>
    <label class="radio-container">Deprivation decile
      <input type="radio" name="grouping" id="grouping-deprivation" value="deprivation">
      <span class="radio-checkmark"></span>
    </label>
    <label class="radio-container">GP CCG
      <input type="radio" name="grouping" id="grouping-gpccg" value="ccg">
      <span class="radio-checkmark"></span>
    </label>
  </div>
</div>

